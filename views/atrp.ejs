<%- include('partials/header') %>
<div class="container mt-5">
  <h1> Attendance Page</h1>

  <div class="row">
    <div class="col-sm-8">
      <div class="card">
        <div class="card-body">
              <table>
                <div class="form-group">
                  <tr>
                    <td>&nbsp;&nbsp;&nbsp;&nbsp;Lecture No.</td>
                    <td>&nbsp;&nbsp;&nbsp;&nbsp;Date </td>
                    <td>&nbsp;&nbsp;&nbsp;&nbsp;Roll Number </td>
                  </tr>
                <% for(var i=0;i<name.length;i++){ %>
                  <div class="form-group">
                <tr>
                  <td>&nbsp;&nbsp;&nbsp;&nbsp;<%=name[i].lno%></td>
                  <td>&nbsp;&nbsp;&nbsp;&nbsp;<%=name[i].date%></td>
                  <td>&nbsp;&nbsp;&nbsp;&nbsp;<%=name[i].rno%></td>
                </tr>
              </div>
            <% } %>
              </table>
              <a href="loginp" class="btn btn-dark">Back</a>
              <a href="calcuted" class="btn btn-dark">Calculated Attedance</a>
        </div>
      </div>
    </div>

  </div>
</div>

<%- include('partials/header') %>
